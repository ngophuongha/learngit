/* ------------ declaring color variables ------------ */
$colorBorder: {{ settings.color_borders }} /*#e6e6e6*/;
$colorPrimary: {{ settings.color_primary }} /*#000*/;
$colorSecondary: {{ settings.color_secondary }} /*#fff*/;

$colorTextBody: {{ settings.color_body_text }} /*#000*/;
$colorBody: {{ settings.color_body_bg }} /*#fff*/;

$colorTextFooter: {{settings.footer_text_color}} /*#000*/;
$colorTextFooterHover: {{settings.footer_text_color_hover}} /*#000*/;
$colorTextFooterTitle: {{settings.footer_text_color_title}} /*#000*/;
$colorBgFooter: {{settings.footer_bg_color}} /*#fff*/;

$colorTextCopyrights: {{settings.cr_text_color}} /*#fff*/;
$colorTextCopyrightsHover: {{settings.cr_text_color_hover}} /*#fff*/;
$colorBgCopyrights: {{settings.cr_bg_color}} /*#000*/;

$colorTextHeaderTop: {{settings.hdt_text_color}} /*#fff*/;
$colorTextHeaderTopHover: {{settings.hdt_text_color_hover}} /*#11b5e6*/;
$colorBgHeaderTop: {{settings.hdt_bg_color}} /*#2c394c*/;

$colorTextHeaderCenter: {{settings.hdc_text_color}} /*#002e5b*/;
$colorTextHeaderCenterHover: {{settings.hdc_text_color_hover}} /*#11b5e6*/;
$colorBgHeaderCenter: {{settings.hdc_bg_color}} /*#fff*/;

$colorTextDropdown: {{settings.color_text_dropdown}} /*#000*/;
$colorTextDropdownHover: {{settings.color_text_dropdown_hover}} /*#000*/;
$colorBgDropdown: {{settings.color_bg_dropdown}} /*#fff*/;

$colorTextHeaderMobile: {{ settings.color_text_hdm }} /*#fff*/;
$colorBgHeaderMobile: {{ settings.color_bg_hdm }} /*#fff*/;

$colorTextNavbar: {{settings.navbar_text_color}} /*#fff*/;
$colorTextNavbarHover: {{settings.navbar_texthover_color}} /*#fff*/;
$colorBgNavbar: {{settings.nav_bg_color}} /*#000*/;

$colorTextSlider: {{settings.color_ms_text}} /*#fff*/;
$colorTextSliderBtn: {{settings.color_ms_btntext}} /*#fff*/;
$colorBgSliderBtn: {{settings.color_ms_btnbg}} /*#11b5e6*/;
$colorBgSliderBtnHover: {{settings.color_ms_btnbg_hover}} /*#2c394c*/;

/* ------------ declaring inheritance ------------ */

%text-12 {
	font-size: 12px;
}

%text-14 {
	font-size: 14px;
}

%text-16 {
	font-size: 16px;
}

%text-18 {
	font-size: 18px;
}

%text-20 {
	font-size: 20px;
}

%text-24 {
	font-size: 24px;
}

%text-bold {
	{% if settings.base_font == "Comfortaa-Regular" %}
	font-weight: bold;
	{%else%}
	font-family: {{settings.base_font}};
	font-weight: bold;
	{%endif%}
}

%text-justify {
	text-align: justify !important;
}

%text-uppercase {
	text-transform: uppercase !important;
}

%smooth-trans {
	-webkit-transition: all 0.3s ease;
	-moz-transition: all 0.3s ease;
	transition: all 0.3s ease;
}

%smooth-trans1 {
	-webkit-transition: all 0.6s ease;
	-moz-transition: all 0.6s ease;
	transition: all 0.6s ease;
}

/* ------------ normalize elements / classes ------------ */

input, button, textarea {
	outline: 0px;
}

.mobile-nav a:focus {
	background: transparent;
}

body {
	margin: 0;
}

body {
	position: relative;
	line-height: 1.6em;

	button,
	h1,
	h2,
	h3 {
		//line-height: normal;
		font-weight: normal;
	}
}

img {
	vertical-align: middle;
}

.mg-bottom-10 {
	margin-bottom: 10px !important;
}

.mg-bottom-30 {
	margin-bottom: 30px !important;
}

.mg-bottom-50 {
	margin-bottom: 50px !important;
}

.mg-left-0 {
	margin-left: 0 !important;
}

.mg-left-5 {
	margin-left: -5px !important;
}

.mg-left-10 {
	margin-left: -10px !important;
}

.mg-left-15 {
	margin-left: -15px !important;
}

.pd-left0 {
	padding-left: 0 !important;
}

.pd-left5 {
	padding-left: 5px !important;
}

.pd-left10 {
	padding-left: 10px !important;
}

.pd-left15 {
	padding-left: 15px !important;
}

@media (max-width: 768px) {
	body .md-mg-left-5 {
		margin-left: -5px !important;
	}

	body .md-pd-left5 {
		padding-left: 5px !important;
	}

	body .md-mg-left-10 {
		margin-left: -10px !important;
	}

	body .md-pd-left10 {
		padding-left: 10px !important;
	}

	body .md-mg-left-15 {
		margin-left: -15px !important;
	}

	body .md-pd-left15 {
		padding-left: 15px !important;
	}
}

input[type=number] {
	-moz-appearance:textfield;
}

input[type=number]::-webkit-inner-spin-button,
input[type=number]::-webkit-outer-spin-button {
	-webkit-appearance: none;
	margin: 0;
}

.fb_iframe_widget iframe {
	max-width: 1000px;
	z-index: 9;
}

.fb-comments,
.fb-comments > span,
.fb-comments span iframe {
	width: 100% !important;
}

.sidebar-sort {
	.no-bullets {
		li {
			input[type="checkbox"] {
				-webkit-appearance: checkbox;
				-moz-appearance: checkbox;
				-webkit-appearance: none;
				-moz-appearance: none;
				appearance: none;
				width: 16px;
				height: 16px;
				background: none;
				border: 1px solid #c3c3c3;
				display: inline-block;
				overflow:hidden;
				vertical-align: middle;
				&:focus{
					outline: none;
				}
				&:checked {
					width: 16px;
					height: 16px;
					&:after{
						content:'\2714';
						font-size:14px;
						line-height:16px;
						text-align:center;
						display:block;
						outline:none;
						color: $colorPrimary;
						outline: 1px solid #fff;
					}
				}
			}
			input[type="radio"] {
				-webkit-appearance: checkbox;
				-moz-appearance: checkbox;
				-webkit-appearance: none;
				-moz-appearance: none;
				appearance: none;
				width: 16px;
				height: 16px;
				background: none;
				border: 1px solid #c3c3c3;
				display: inline-block;
				overflow:hidden;
				vertical-align: middle;
				&:focus{
					outline: none;
				}
				&:checked {
					width: 16px;
					height: 16px;
					&:after{
						content:'\2714';
						font-size:14px;
						line-height:16px;
						text-align:center;
						display:block;
						outline:none;
						color: $colorPrimary;
						outline: 1px solid #fff;
					}
				}
			}
		}
	}
}

.pagination-custom {
	> span {
		display: inline-block;
		height: 40px;
		width: 40px;
		line-height: 40px;
		margin: 5px;
		border-radius: 5px;
		&.current {
			padding: 0px !important;
			@extend %text-18;
			@extend %text-bold;
			color: #fff;
			background: $colorPrimary;
			border: 1px solid $colorPrimary;
		}
		> a {
			border-radius: 5px;
			padding: 0px;
			display: inline-block;
			text-align: center;
			height: 40px;
			line-height: 40px;
			width: 40px;
			padding: 0px;
			@extend %text-18;
			@extend %text-bold;
			@extend %smooth-trans;
			color: $colorTextBody;
			border: 1px solid #f3f3f3;
			background: #f8f8f9;
			&:hover {
				color: #fff;
				background: $colorPrimary;
				border: 1px solid $colorPrimary;
			}
		}
	}
}

#pagination-page {
	clear: left;
	margin: 0px;
}

.pagination {
	clear: left;
	margin: 0px;
}

.float-right {
	float: right;
}

input.btn {
	display: inline-block;
	padding: 5px 15px;
	border-radius: 5px;
	background: $colorPrimary;
	color: #fff;
	@extend %text-bold;
	@extend %smooth-trans;
	&:hover {
		background: darken($colorPrimary, 10%);
	}
}

.section-title {
	margin-bottom: 30px;
	h2 {
		@extend %text-24;
		color: $colorPrimary;
		margin: 0px;
	}
	.section-title-border {
		display: inline-block;
		height: 5px;
		width: 100px;
		background: $colorPrimary;
	}
}

.swatch .header {
	@extend %text-bold;
}

.js-qty {
	margin: 0px;
	max-width: 150px;
	input, button {
		outline: 0px;
	}
}

.qty-addcart > span {
	margin: 0;
	font-weight: bold;
	margin-bottom: 5px;
	display: block;
	min-width: 200px;
	position: relative;
}

#hrv-product-reviews .title-bl h2 {
	@extend %text-14;
	@extend %text-bold;
	color: $colorTextBody;
	padding: 0px;
	margin: 0px 0px 15px 0px;
}

#btnSubmitReview, #btnnewreview {
	border-radius: 5px;
	outline: 0px;
	padding: 5px 15px;
	background: $colorPrimary !important;
	color: #fff !important;
	@extend %text-bold;
	cursor: pointer;
}

.search-h1 {
	@extend %text-18;
	@extend %text-bold;
	@extend %text-uppercase;
}

.search-item .product-item {
	margin-bottom: 30px;
}
/* ------------ breakpoints ------------ */
@media (min-width: 768px) {
	#ProductPhoto {
		float: right;
		width: calc(100% - 92px);
	}
	#ProductThumbs {
		float: left;
		max-width: 77px;
		overflow-y: hidden;
		max-height: 480px;
		position: relative;
	}
}
@media (max-width: 480px) {
	#modalAddComplete {
		.modal-checkout-actions {
			.text-left a {
				font-size: 12px;
			}
			h3 {
				margin-top: 5px !important;
			}
			.you-save {
				margin-bottom: 5px !important;
			}
			.modal-cart-actions a {
				margin-top: 0px !important;
			}
		}
	}

	#product-wrapper .product-description-wrapper div.tab button {
		width: 100% !important;
	}
	.product-thumb-control {
		display: none !important;
	}
	#blog-wrapper .article-content .article-body div.tab button {
		width: 100% !important;
	}
	#blog-wrapper .article-content .article-head h1, #blog-wrapper .related-articles .related-articles-head h3 {
		font-size: 18px !important;
	}
	.cd-horizontal-timeline .events-content li img {
		margin-bottom: 15px;
	}
	body {
		padding-bottom: 67px;
	}
}
