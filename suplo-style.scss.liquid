/* -------- MAIN SLIDER -------- */
@media (max-width: 768px) {
	#hero-slider .hero-slide {
		background-attachment: scroll !important;
		.hero-slide-content {
			width: 100% !important;
			h4 {
				font-size: 36px !important;
			}
		}
	}
}
#hero-slider {
	margin-bottom: 30px;
	.slick-dots {
		position: absolute;
		z-index: 1;
		left: 0;
		right: 0;
		bottom: 15px !important;
		> li > button {
			border: 2px solid #fff;
			border-radius: 50%;
			&:before {
				font-size: 30px;
				color: #fff;
			}
		}
	}
	.slick-prev {
		left: 15px !important;
		z-index:9999;
		height: 50px;
		line-height: 50px;
		width: 50px;
		background: rgba($colorPrimary,0.7);
		padding: 0px;
		margin: 0px;
	}
	.slick-next {
		right: 15px !important;
		z-index:9999;
		height: 50px;
		line-height: 50px;
		width: 50px;
		background: rgba($colorPrimary,0.7);
		padding: 0px;
		margin: 0px;
	}
	.hero-slide {
		background-size: cover;
		background-position: center;
		background-repeat: no-repeat;
		background-attachment: fixed;
		position: relative;
		.hero-slide-overlay {
			position: absolute;
			z-index: -1;
			top: 0;
			right: 0;
			bottom: 0;
			left: 0;
			background: rgba(0,0,0,0.2);
		}
		.hero-slide-content {
			position: absolute;
			top: 50%;
			left: 50%;
			-webkit-transform: translate(-50%, -50%);
			transform: translate(-50%, -50%);
			width: 60%;
			padding: 30px;
			.hero-content {
				.slide-message {
					h4 {
						font-size: 48px;
						font-weight: bold;
						@extend %text-bold;
						color: $colorTextSlider;
						margin: 0px 0px 15px 0px;
					}
					p {
						font-size: 16px;
						color: $colorTextSlider;
					}
				}
				.slide-button > a {
					display: inline-block;
					padding: 5px 15px;
					border-radius: 5px;
					background: $colorBgSliderBtn;
					color: $colorTextSliderBtn;
					@extend %text-bold;
					@extend %smooth-trans;
					&:hover {
						background: $colorBgSliderBtnHover;
					}
				}
			}
		}
	}
}
