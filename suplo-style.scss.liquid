	#page-wrapper{
		.cart{
			.cart-item{
				margin-bottom:15px;
				border-bottom:1px solid $colorBorder;
				.h4{
					font-weight:normal !important;
					font-size:14px !important;
					margin-bottom:0px;
				}
				.price{
					display:inline-block;
					.h3{
						font-size:16px !important;
