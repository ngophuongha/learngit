	height: 30px;
	width: 30px;
	padding: 0;
	margin: 0px;
	background: $colorPrimary;
	color: #fff;
	opacity: 0.8;
	z-index: 9;
	@extend %smooth-trans;
	&:hover {
		opacity: 1;
	}
	&.up {
		top: 0;
	}
	&.down {
		bottom: 0;
	}
}
.product-buynow-sidebar {
	padding: 15px;
	background: #f9f9f9;
	border-radius: 5px;
	h3 {
		@extend %text-bold;
		@extend %text-16;
		color: $colorPrimary;
		margin: 0px 0px 5px 0px;
	}
	.pro-sku {
		@extend %text-12;
		margin-bottom: 15px;
	}
	.product-buynow-img {
		margin-bottom: 15px;
		a {
			display: block;
			img {
				width: 100%;
			}
		}
	}
	.product-buynow-price {
		margin-bottom: 15px;
		> span {
			display: inline-block;
			&.current-price {
				font-weight: bold;
				color: $colorPrimary;
				@extend %text-18;
			}
			&.original-price {
				margin-left: 10px;
			}
		}
	}
	.product-buynow-actions {
		button {
			display: block;
			width: 100%;
			background: $colorPrimary;
			color: #fff;
			border-radius: 5px;
			padding: 10px;
			@extend %text-16;
			@extend %text-bold;
			@extend %smooth-trans;
			&:hover {
				background: darken($colorPrimary,10%);
			}
		}
	}
}
@media (max-width:768px) {
	#related-products {
		#owl-related-products-slider .owl-controls .owl-buttons > div {
			opacity: 1 !important;
		}
	}
	#seen-products {
		#owl-spdx .owl-controls .owl-buttons > div {
			opacity: 1 !important;
		}
	}
}
#related-products {
	overflow: hidden;
	margin-bottom: 30px;
	#owl-related-products-slider:hover .owl-controls .owl-buttons > div {
		opacity: 1;
	}
	.owl-controls {
		margin: 0px;
		.owl-buttons > div {
			position: absolute;
			top: calc(50% - 17px);
			padding: 0px;
			margin: 0px;
			width: 34px;
			height: 34px;
			line-height: 34px;
			background: rgba($colorPrimary, 0.7);
			color: #fff;
			border-radius: 0px;
			@extend %smooth-trans;
			@extend %text-18;
			opacity: 0;
			border-radius: 3px;
			&.owl-prev {
				left: 15px;
			}
			&.owl-next {
				right: 0;
			}
			&:hover {
				background: rgba($colorPrimary, 1);
			}
		}
	}
}
#seen-products {
	overflow: hidden;
	#owl-spdx:hover .owl-controls .owl-buttons > div {
		opacity: 1;
	}
	#owl-spdx {
		.owl-controls {
			margin: 0px;
			.owl-buttons > div {
				position: absolute;
				top: calc(50% - 17px);
				padding: 0px;
				margin: 0px;
				width: 34px;
				height: 34px;
				line-height: 34px;
				background: rgba($colorPrimary, 0.7);
				color: #fff;
				border-radius: 0px;
				@extend %smooth-trans;
				@extend %text-18;
				opacity: 0;
				border-radius: 3px;
				&.owl-prev {
					left: 15px;
				}
				&.owl-next {
					right: 0;
				}
				&:hover {
					background: rgba($colorPrimary, 1);
				}
			}
		}
	}
}

