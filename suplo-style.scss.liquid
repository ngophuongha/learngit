// ---------- NAV DRAWER ---------- //
#NavDrawer::-webkit-scrollbar {
	width: 7px;
}
#NavDrawer::-webkit-scrollbar-track {
	-webkit-box-shadow: inset 0 0 5px #fff; 
	border-radius: 0px;
}
#NavDrawer::-webkit-scrollbar-thumb {
	border-radius: 0px;
	-webkit-box-shadow: inset 0 0 5px #fff; 
	background: #fff;
}
#NavDrawer {
	z-index: 999999;
	background: rgba($colorBgNavbar,0.9);
	padding: 0px;

	.mobile-nav > .mobile-nav__item {
		background: transparent;
	}

	.drawer__header {
		height: 65px;
		border-bottom: 0px;
		padding: 15px 15px 0px 15px;
		.drawer__close {
			text-align: right;
			button {
				right: 0px;
				@extend %text-14;
				@extend %smooth-trans;
				@extend %text-uppercase;
				color: $colorTextNavbar;
				background: transparent;
				outline: 0px;
				border: 0px;
				border-radius: 0px;
				padding-right: 0px;
				svg {
					font-size: 36px;
				}
			}
		}
	}

	ul.mobile-nav {
		margin: 0px;
		> li {
			&.mobile-nav__search {
				padding: 15px !important;
				form {
					border-radius: 100px;
					overflow: hidden;
					input {
						height: 34px;
						line-height: 34px;
						padding: 0px 15px;
						border-radius: 0px;
						border: 0px;
						outline: 0px;
						width: 100%;
						background: #fff;
					}
					button {
						height: 34px;
						width: 34px;
						line-height: 34px;
						padding: 0px;
						border-radius: 0px;
						border: 0px;
						outline: 0px;
						background: #fff;
						color: $colorPrimary;
						@extend %text-18;
					}
				}
			}

			&.mobile-nav__item {
				padding: 0px;
				display: block;
				padding: 15px;
				background: rgba($colorBgNavbar,0);
				@extend %smooth-trans;
				border-left: 3px solid transparent;

				> .mobile-nav__has-sublist {
					> a {
						width: auto !important;
						display: inline-block;
						position: relative;
						padding: 0px;
						@extend %text-16;
						@extend %text-uppercase;
						@extend %smooth-trans;
						color: $colorTextNavbar;			
						&:after {
							content: '';
							display: block;
							border-bottom: 1px solid $colorTextNavbarHover;
							width: 0;
							position: absolute;
							left: 0;
							-webkit-transition: 1s ease;
							transition: 1s ease;
						}
						&:hover {
							opacity: 1 !important;
							color: $colorTextNavbarHover;
							&:after {
								width: 100%;
							}
						}
					}
					.mobile-nav__toggle button {
						padding: 0px;
						color: $colorTextNavbar;
						background: transparent;
						outline: 0px;
						border-radius: 0px;
						border: 0px;
						&:hover {
							color: $colorTextNavbarHover;
						}
					}
				}
				> .mobile-nav__sublist {
					padding-left: 15px;
					> .mobile-nav__item {
						padding: 15px 0px;
						&:first-child {
							padding-top: 30px;
						}
						&:last-child {
							padding-bottom: 0px;
						}
						> .mobile-nav__has-sublist {
							> a {
								display: inline-block;
								width: auto !important;
								position: relative;
								padding: 0px;
								@extend %text-16;
								@extend %text-uppercase;
								@extend %smooth-trans;
								color: $colorTextNavbar;			
								&:after {
									content: '';
									display: block;
									border-bottom: 1px solid $colorTextNavbarHover;
									width: 0;
									position: absolute;
									left: 0;
									-webkit-transition: 1s ease;
									transition: 1s ease;
								}
								&:hover {
									opacity: 1 !important;
									color: $colorTextNavbarHover;
									&:after {
										width: 100%;
									}
								}
							}
							.mobile-nav__toggle button {
								padding: 0px;
								color: $colorTextNavbar;
								background: transparent;
								outline: 0px;
								border-radius: 0px;
								border: 0px;
								&:hover {
									color: $colorTextNavbarHover;
								}
							}
						}
						> .mobile-nav__sublist {
							padding-top: 15px;
							> .mobile-nav__item {
								padding: 15px 0px 15px 15px;
								> a {
									display: inline-block;
									width: auto !important;
									position: relative;
									padding: 0px;
									@extend %text-16;
									@extend %text-uppercase;
									@extend %smooth-trans;
									color: $colorTextNavbar;			
									&:after {
										content: '';
										display: block;
										border-bottom: 1px solid $colorTextNavbarHover;
										width: 0;
										position: absolute;
										left: 0;
										-webkit-transition: 1s ease;
										transition: 1s ease;
									}
									&:hover {
										opacity: 1 !important;
										color: $colorTextNavbarHover;
										&:after {
											width: 100%;
										}
									}
								}
							}
						}
					}
				}


				&:hover {
					background: rgba($colorBgNavbar,1);
					border-left: 3px solid $colorTextNavbarHover;
					> a {
						&:after {
							width: 100%;
						}
					}
				}
			}

			> a {
				display: inline-block;
				position: relative;
				padding: 0px;
				@extend %text-16;
				@extend %text-uppercase;
				@extend %smooth-trans;
				color: $colorTextNavbar;			
				&:after {
					content: '';
					display: block;
					border-bottom: 1px solid $colorTextNavbarHover;
					width: 0;
					position: absolute;
					left: 0;
					-webkit-transition: 1s ease;
					transition: 1s ease;
				}
				&:hover {
					color: $colorTextNavbarHover;
					opacity: 1 !important;
				}
			}

			&:after {
				display: none;
			}
		}
	}
}
// ---------- POP UP SUBSCRIBE ---------- //
.popup {
	display: none;
	/* Hidden by default */
	position: fixed;
	/* Stay in place */
	z-index: 999999999;
	/* Sit on top */
	left: 0;
	top: 0;
	width: 100%;
	/* Full width */
	height: 100%;
	/* Full height */
	overflow: auto;
	/* Enable scroll if needed */
	background-color: rgb(0,0,0);
	/* Fallback color */
	background-color: rgba(0,0,0,0.8);
	/* Black w/ opacity */

	.popup-content {
		background: url('{{'popup_bg.png' | asset_url}}');
		background-position: center;
		background-repeat: no-repeat;
		background-size: cover;
		margin: auto;
		max-width: 750px;
		width: 90%;
		top: 50px;
		position: relative;

		.popup-wrapper {
			background: rgba(255, 255, 255, 0.8);
			padding: 30px 15px;
		}

		.popup-title {
			@extend %text-18;
			@extend %text-bold;
			color: $colorPrimary;
			margin-bottom: 15px;
		}

		.popup-desc {
			color: $colorTextBody;
			margin-bottom: 15px;
			text-align: justify;
		}

		.popup-list {
			margin-bottom: 15px;

			ul {
				margin: 0;

				li {
					position: relative;
					margin-bottom: 5px;
					padding-left: 15px;

					&:last-child {
						margin: 0 !important;
					}

					&:after {
						content: '\f111';
						font-family: FontAwesome;
						position: absolute;
						top: calc(50% - 3px);
						left: 0;
						font-size: 6px;
						color: $colorPrimary;
						line-height: normal !important;
					}
					color: $colorTextBody;
				}
			}
		}

		.form-desc {
			@extend %text-16;
			color: $colorTextBody;
			margin-bottom: 15px;
		}

		form {
			position: relative;
			width: 100%;

			.input-group {
				width: 100%;
			}

			input {
				width: 100%;
				height: 40px;
				line-height: 40px;
				padding: 0px 45px 0px 15px;
				@extend %text-14;
				color: $colorTextBody;
				background: #fff;
				border: 1px solid $colorPrimary;
				border-radius: 100px !important;
				outline: 0 !important;
				margin-bottom: 15px;
			}

			button {
				position: absolute;
				top: 3px;
				right: 3px;
				height: 34px;
				width: 34px;
				border-radius: 50%;
				display: inline-block;
				background: $colorPrimary;
				color: #fff;
				@extend %text-18;
				@extend %text-bold;
				@extend %text-uppercase;
				@extend %smooth-trans;
				&:hover {
