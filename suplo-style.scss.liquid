/* ------- BLOG - ARTICLE ------------ */
#blog-wrapper {
	padding: 30px 0px;
	.blog-content {
		.section-title {
			margin-bottom: 30px;
			height: 24px;
			line-height: 24px;

			.title {
				float: left;
				display: inline-block;

				h2 {
					display: inline-block;
					margin: 0;
					padding: 0 15px 0 0;
					background: #fff;
					@extend %text-18;
					@extend %text-bold;
					color: $colorPrimary;
					position: relative;
					z-index: 9;
				}
			}

			.viewmore {
				float: right;
				display: inline-block;

				a {
					display: inline-block;
					padding: 0 0 0 15px;
					background: #fff;
					color: $colorTextBody;
					position: relative;
					z-index: 9;
					-webkit-transition: all 0.3s ease;
					-moz-transition: all 0.3s ease;
					transition: all 0.3s ease;
					&:hover {
						color: $colorPrimary;
					}
				}
			}

			.line {
				height: 1px;
				width: 100%;
				background: $colorBorder;
				transform: translateY(10px);
			}
		}
		.banner-horizontal {
			margin-bottom: 30px;
			a {
				display: block;
				overflow: hidden;
				border-radius: 5px;
				img {
					width: 100%;
					vertical-align: middle;
					-webkit-transition: all 0.3s ease;
					-moz-transition: all 0.3s ease;
					transition: all 0.3s ease;
					transform: scale(1);
					&:hover {
						transform: scale(1.05);
					}
				}
			}

		}
		.blog-single-item {
			margin-bottom: 30px;
			&:hover {
				.owl-buttons {
					> div {
						display: inline-block !important;
					}
				}
			}

			.owl-controls {
				margin: 0px;
				.owl-buttons {
					> div {
						position: absolute;
						background: $colorPrimary !important;
						opacity: 0.6 !important;
						height: 30px;
						line-height: 30px;
						width: 30px;
						top: calc(50% - 15px);
						font-size: 18px;
						color: #fff;
						padding: 0px;
						margin: 0px;
						border-radius: 3px;
						outline: 0px;
						&:hover {
							opacity: 1 !important;
						}
						&.owl-prev {
							left: 30px;
						}
						&.owl-next {
							right: 0px;
						}
					}
				}
			}
		}
		.blog-content-wrapper {
			.blog-head {
				margin-bottom: 30px;
				h1 {
					@extend %text-24;
					@extend %text-bold;
					color: $colorPrimary;
				}
			}
			.blog-body {
				.grid-uniform .article-item {
					margin-bottom: 30px;
				}
			}
		}
	}
	.blog-sidebar {
		> div {
			margin-bottom: 30px;
		} 
		.blog-sb-title h3 {
			@extend %text-18;
			background: $colorPrimary;
			color: #fff;
			margin: 0px 0px 5px 0px;
			padding: 15px;
			border-top-left-radius: 5px;
			border-top-right-radius: 5px;
			-webkit-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1);
			-moz-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1);
			box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1);
			text-align: center;
		}
		.list-categories > ul {
			margin: 0px;
			li {
				margin: 0px 0px 5px 0px;
				-webkit-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1);
				-moz-box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1);
				box-shadow: 0 1px 1px 0 rgba(0, 0, 0, 0.1);
				&:last-child {
					margin: 0px;
					border-bottom-left-radius: 5px;
					border-bottom-right-radius: 5px;
				}

				&.active {
					a {
						background: $colorPrimary;
						color: #fff;
						.isb-cate-icon {
							background: $colorPrimary;
							svg {
								color: #fff;
							}
						}
					}
				}

				a {
					position: relative;
					display: block;
					background: #f9f9f9;
					color: $colorPrimary;
					@extend %smooth-trans;
					@extend %text-14;
					padding: 15px 15px 15px 50px;

					&:hover {
						background: $colorPrimary;
						color: #fff;
						.isb-cate-icon {
							background: $colorPrimary;
							svg {
								color: #fff;
							}
						}
					}

					.isb-cate-icon {
						display: block;
						background: #fff;
						position: absolute;
						height: 100%;
						top: 0;
						left: 0;
						padding: 15px;
						@extend %smooth-trans;
						svg {
							display: inline-block;
							@extend %text-20;
							@extend %smooth-trans;
							color: $colorPrimary;
						}
					}
				}
			}
		}

		.all-tags .all-tags-wrapper {
			a {
				float: left;
				display: inline-block;
				border: 1px solid #f3f3f3;
				background: #f8f8f9;
				padding: 10px;
				margin: 0px 3px 3px 0px;
				@extend %smooth-trans;
				color: $colorTextBody;
				&:hover {
					border: 1px solid $colorPrimary;
					color: #fff;
					background: $colorPrimary;
				}
			}
		}

		.blog-sb-banner {
			a {
				border-radius: 5px;
				overflow: hidden;
				display: block;
				overflow: hidden;
				img {
					@extend %smooth-trans;
					width: 100%;
					transform: scale(1);
					&:hover {
						transform: scale(1.05);
					}
				}
			}
		}
	}
	.article-content {
		.article-head {
			position: relative;
			width: 100%;
			text-align: left;
			display: block;
			margin-bottom: 30px;
			h1 {
				@extend %text-24;
				@extend %text-bold;
				color: $colorPrimary;
				padding: 0px;
				margin: 0px 0px 5px 0px;
			}
			.article-date-comment {
				> div {
					display: inline-block;
					margin-right: 15px;
					svg {
						color: $colorPrimary;
					}
				}
			}
		}
		.article-tldr {
			padding: 15px 15px 0px 15px;
			margin-bottom: 30px;
			background: #f9f9f9;
			border-radius: 5px;
		}
		.article-tags {
			a {
				display: inline-block;
				border: 1px solid #f3f3f3;
				background: #f8f8f9;
				padding: 10px;
				margin: 0px 3px 3px 0px;
				@extend %smooth-trans;
				color: $colorTextBody;
				&:hover {
					border: 1px solid $colorPrimary;
					color: #fff;
					background: $colorPrimary;
				}
			}
		}
	}
	.social-network-actions-outside {
		padding: 15px 0px 15px 15px;
	}
	#section-fbcomment {
		margin-bottom: 30px;
	}
	.related-articles {
		margin-bottom: 30px;
		.related-articles-head {
			position: relative;
			width: 100%;
			text-align: left;
			display: block;
			h3 {
				padding: 0px;
				margin: 0px 0px 5px 0px;
				@extend %text-24;
				@extend %text-bold;
				color: $colorPrimary;
			}
		}
		.related-articles-body {
			ul {
				margin: 0px;
				> li {
					display: block;
					> a {
						position: relative;
						display: inline-block;
						padding-left: 15px;
						@extend %smooth-trans;
						color: $colorTextBody;

						&:hover {
							color:  $colorPrimary;
							&:before {
								color: $colorPrimary;
							}
						}

						&:before {
							content: '\f111';
							font-family: FontAwesome;
							position: absolute;
							top: calc(50% - 3px);
							left: 0;
							font-size: 6px;
							color: $colorTextBody;
							@extend %smooth-trans;
							line-height: normal;
						}
					}
				}
			}
		}
	}
}
/* ------- MOBILE BOTTOM NAVIGATION ------------ */
#mobile-bottom-navigation {
	position: fixed;
	bottom: 0;
	width: 100%;
	z-index: 99999;
	border-top: 1px solid $colorBorder;

	.mobile-nav-item {
		display: block;
		height: 67px;
		padding: 0;
		text-align: center;
		background: $colorPrimary;
		position: relative;

		> a {
			width: 100%;
			position: absolute;
			top: 50%;
			left: 50%;
			-webkit-transform: translate(-50%, -50%);
			transform: translate(-50%, -50%);
			font-size: 12px;
			color: #fff;

			svg {
				font-size: 24px;
				color: #fff;
				display: inline-block;
			}

			span.number {
				position: absolute;
				display: inline-block;
				top: -5px;
				right: 15px;
				height: 20px;
				line-height: 20px;
				width: 20px;
				border-radius: 50%;
				text-align: center;
				font-size: 14px;
				color: #fff;
				background: $colorSecondary;
			}
		}
	}
}
/* ------- FIXED SOCIAL NETWORK ------------ */
#fixed-social-network {
	position: fixed;
	top: 25%;
	right: -125px;
	z-index: 99999;
	> a {
		border-radius: 3px;
		height: 40px;
		line-height: 40px;
		padding: 0px 20px 0px 15px;
		display: block;
		width: 170px;
		@extend %text-12;
		@extend %text-bold;
		@extend %smooth-trans1;
		transform: translateX(0px);
		margin-bottom: 5px;
		> svg {
			margin-right: 20px;
			@extend %text-18;
		}
		&:hover {
			transform: translateX(-120px);
		}
		&.fb-icon {
			background: #3B5998;
			color: #fff;
		}
		&.ins-icon {
			background: #C32AA3;
			color: #fff;
		}
		&.yt-icon {
			background: #FF0000;
			color: #fff;
		}
		&.tw-icon {
			background: #00B6F1;
			color: #fff;
		}
		&.gg-icon {
			background: #DF4A32;
			color: #fff;
		}
		&.linkedin-icon {
			background: #0077b5;
			color: #fff;
		}
		&.skype-icon {
			background: #15ace5;
			color: #fff;
		}
		&.zalo-icon {
			background: #41a0d7;
			color: #fff;
		}
		&#back-to-top {
			background: $colorPrimary;
			color: #fff;
		}
	}
}
